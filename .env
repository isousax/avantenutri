###############################################
# FRONT-END (.env para Vite)
###############################################

# Base do site (para comparar iss/aud)
VITE_SITE_DNS=https://avantenutri.com.br

# URL JWKS do login-service (RS256)
VITE_JWT_JWKS_URL=https://login-service.avantenutri.workers.dev/auth/.well-known/jwks.json

# issuer / audience esperados (se vazio, aceita qualquer)
VITE_JWT_EXPECTED_ISSUER=https://avantenutri.com.br
VITE_JWT_EXPECTED_AUDIENCE=https://avantenutri.com.br

# TTL cache /api/auth/me (ms)
VITE_ME_CACHE_TTL_MS=60000

# Versão da aplicação (para exibir em footer / debug)
VITE_APP_VERSION=1.0.0

###############################################
# FIM
###############################################
